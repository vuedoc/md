{
  "name": "@vuedoc/md",
  "version": "3.2.0",
  "description": "Generate a Markdown Documentation for a Vue file component",
  "main": "index.js",
  "bin": {
    "vuedoc.md": "./bin/cli.js"
  },
  "scripts": {
    "test": "jest --coverage",
    "lint": "eslint index.js bin lib",
    "lint:fix": "eslint --fix index.js bin lib test",
    "install:dev": "npm i && npm i --no-save @vuedoc/parser",
