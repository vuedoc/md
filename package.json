    "url": "https://gitlab.com/vuedoc/md/issues"
  },
  "homepage": "https://gitlab.com/vuedoc/md",
  "dependencies": {
    "@textlint/markdown-to-ast": "^6.3.1",
    "ast-to-markdown": "^1.0.0",
    "deepmerge": "^4.2.2",
    "jsonschemav": "^1.0.7",
    "markdown-table": "^2.0.0",
    "md-node-inject": "^1.0.1",
    "mdast-util-to-string": "^2.0.0",
    "word-wrap": "^1.2.3"
